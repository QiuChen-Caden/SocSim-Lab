# SocSim Lab Frontend - Environment Variables

# Use real backend API (true) or mock data (false)
VITE_USE_REAL_API=false

# Backend API URL (only used when VITE_USE_REAL_API=true)
VITE_API_URL=http://localhost:8000

# WebSocket URL (only used when VITE_USE_REAL_API=true)
VITE_WS_URL=ws://localhost:8000/ws

# Enable realtime websocket stream (set false to use HTTP polling only)
VITE_USE_WEBSOCKET=true
